{
  "name": "<%= name %>",
  "private": true,
  "version": "1.0.0",
  "workspaces": [
    "packages/*"
  ],
  "description": "<%= description %>",
  "scripts": {
    "lint": "turbo run lint --parallel",
    "lint:fix": "turbo run lint --parallel -- --fix",
    "lint:affected": "turbo run lint --parallel --filter=...[origin/<%= main %>]",
    "test": "turbo run test --parallel",
    "test:affected": "turbo run test --parallel --filter=...[origin/<%= main %>]",
    "build": "turbo run build --stream",
    "dev": "turbo run dev --parallel",
    "update-deps": "npx npm-check-updates -u --deep"
  },
  "engines": {
    "node": ">=v16.16.0",
    "npm": ">=8.14.0"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^5.30.6",
    "@typescript-eslint/parser": "^5.30.6",
    "eslint": "^8.19.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.2.1",
    "prettier": "^2.7.1",
    "turbo": "^1.3.1"
  }
}
